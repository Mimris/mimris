{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm install --force && npm rebuild"
            }
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
